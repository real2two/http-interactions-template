{
  "name": "@/example",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "main": "src/http/clusters.ts",
  "scripts": {
    "start": "dotenv -e ../../.env -- tsx .",
    "dev": "nodemon",
    "format": "cd ../../ && pnpm format",
    "commands/create": "dotenv -e ../../.env -- tsx tools/createCommands.ts"
  },
  "nodemonConfig": {
    "ignore": [".git", "node_modules"],
    "watch": ["../../"],
    "exec": "npm start",
    "ext": "ts, js"
  },
  "dependencies": {
    "@/env": "workspace:*",
    "@discordjs/core": "^1.2.0",
    "@discordjs/rest": "^2.3.0",
    "@httpi/adapter-hyperexpress": "workspace:*",
    "@httpi/client": "workspace:*",
    "dotenv-cli": "^7.4.2",
    "hyper-express": "^6.16.3",
    "tsx": "^4.14.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@/tsconfig": "workspace:*",
    "@types/node": "^20.14.2",
    "discord-api-types": "^0.37.87",
    "nodemon": "^3.1.3"
  }
}
